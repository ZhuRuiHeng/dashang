page{
  background: #000;
}
.zan-dialog__container { bottom:168px; left:36px; width:288px; text-align:center; height:268px; border-radius: 7px;
z-index: 6; }
.contain{ display:-webkit-flex; display:flex;-webkit-flex-direction:column;flex-direction:column;z-index:5;-webkit-align-items:center;align-items:center;-webkit-justify-content:center;justify-content:center; }
.notification .notification-inner{
    padding:20rpx;display: flex;justify-content:flex-start;color:#fff;
    font-size:26rpx;
}
.notification .notification-media .notification-image{
  padding-top: 0.1rem;
  margin-right: 10rpx;
  width: 30rpx;
  height: 30rpx;
}
.notification-content{
  display: inline-block;
}
.notification{background-color: #540000; }
.notification .notification-text { line-height:1rem; }
button{
  border: none;
  color: #353535;
  background-color: transparent;
}
button:after{
  content: none;
}
.zan-dialog__mask {
   display: block;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
    background: rgba(0, 0, 0, 0.7);
}
.zan-dialog__container{
    width: 80%;
    background-color: #f1f1f1;
    text-align: center;
    border-radius: 7px;
    z-index: 11;
    position:absolute;
    top:50%;
    left: 50%;
   -webkit-transform: translate(-50%,-50%);  
   -ms-transform: translate(-50%,-50%);
   transform: translate(-50%,-50%);
}
button.formidBtn.button-hover{
  background: none;
}
video{
  width:350px;height:467px;display: block;z-index: 5;
}
.blur {    
    -webkit-filter: blur(15px);
    -moz-filter: blur(15px);
    -o-filter: blur(15px);
    -ms-filter: blur(15px);
    filter: blur(15px);   
}
.false{
  position: absolute;
  top: 0;
  left: 0;
  bottom: 92rpx;
  width:100%;height:1000rpx;display: block;z-index:8;opacity:0.2;
  background: rgba(0,0,0,0.1);
}